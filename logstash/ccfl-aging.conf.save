input {
    file {
        path => "/home/ubuntu/ccfl-data/Respite-Caregiver-Demographics/2016-01.csv"
        start_position => "beginning"
    }
}

filter {
    csv {
        separator => ","
        columns => ["Client ID","Race","Gender","Marital Status","County","Age","Date"]        mutate {
	    convert => {
              "Client ID" => "integer"
              "Age" => "integer"
            }
	}
        date {
            match => ["Date", "MM-yyyy"]
            target => "Date"
        }
    }
}

output {
   elasticsearch {
     hosts => "http://localhost:9200"
     index => "ccfl-aging"
  }
stdout {}
}
